{
  rateLimit {
    cost
    limit
    nodeCount
    remaining
    resetAt
  }
  organization(login: "SoapBox") {
    members(last: 100) {
      nodes {
        id
        name
        login
        avatarUrl
      }
    }
    repositories(first: 20, orderBy: {field: PUSHED_AT, direction: DESC}) {
      nodes {
        name
        pullRequests(first: 50, orderBy: {field: UPDATED_AT, direction: DESC}, states: OPEN) {
          nodes {
            id
            url
            title
            author {
              login
              avatarUrl
              ... on User {
                id
                name
              }
            }
            reviewRequests(last: 10) {
              nodes {
                requestedReviewer {
                  __typename
                  ... on User {
                    id
                    login
                    name
                    avatarUrl
                  }
                }
              }
            }
            reviews(states: [APPROVED, CHANGES_REQUESTED, DISMISSED], last: 10) {
              nodes {
                state
                submittedAt
                author {
                  login
                  avatarUrl
                  ... on User {
                    name
                    id
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
